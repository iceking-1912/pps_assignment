    "    num_list.append(num)\n",
    "sum=0\n",
    "for i in range(0,N):\n",
    "    sum+= num_list[i]\n",
    "\n",
    "avg = sum/N\n",
    "\n",
    "print(num_list)\n",
    "print(max(num_list))\n",
    "print(min(num_list))\n",
    "print(sum)\n",
    "print(avg)\n"
